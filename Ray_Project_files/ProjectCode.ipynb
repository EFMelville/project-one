  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Get all the csv files in one file\n",
    "csv_files = glob.glob(\"resources/*.csv\")\n",
    "\n",
    "#Create a list to store the csv file data frames.\n",
    "csv_data = {}\n",
    "\n",
    "#Loop to read all data frames and create each data frame in the 'csv_data' dictionary\n",
    "for filename in csv_files:\n",
    "    #Use os.path to get the base name of the file\n",
    "    basename = os.path.basename(filename)\n",
    "    #remove the .csv extension from the filename\n",
    "    name = os.path.splitext(basename)[0]\n",
    "    #Read csv file into a DataFrame and store it in the dictionary\n",
    "    csv_data[name] = pd.read_csv(filename)"
   ]
  },
